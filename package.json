{
  "name": "fastify-zod-openapi",
  "version": "0.1.0",
  "description": "Fastify plugin for zod-openapi",
  "keywords": [
    "typescript",
    "json-schema",
    "swagger",
    "openapi",
    "openapi3",
    "zod",
    "zod-openapi",
    "fastify",
    "type",
    "provider"
  ],
  "homepage": "https://github.com/samchungy/fastify-zod-openapi#readme",
  "bugs": {
    "url": "https://github.com/samchungy/fastify-zod-openapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/samchungy/fastify-zod-openapi.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./lib-commonjs/index.js",
  "module": "./lib-es2015/index.js",
  "types": "./lib-types/index.d.ts",
  "files": [
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "lib*/**/*.json"
  ],
  "scripts": {
    "build": "skuba build-package",
    "format": "skuba format",
    "lint": "skuba lint",
    "prepare": "yarn build",
    "release": "yarn build && npm publish --provenance",
    "start": "skuba node --port=5000 examples/openapi.ts",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch"
  },
  "dependencies": {
    "fastify-plugin": "^4.5.0"
  },
  "devDependencies": {
    "@fastify/swagger": "8.6.0",
    "@fastify/swagger-ui": "1.9.0",
    "@types/node": "20.3.0",
    "eslint-plugin-zod-openapi": "^0.1.0",
    "fastify": "4.19.1",
    "skuba": "6.2.0",
    "zod": "3.21.4",
    "zod-openapi": "2.6.0"
  },
  "peerDependencies": {
    "zod": "^3.21.4",
    "zod-openapi": "^2.6.0",
    "fastify": "4"
  },
  "engines": {
    "node": ">=16.11"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "skuba": {
    "entryPoint": "src/index.ts",
    "template": "oss-npm-package",
    "type": "package",
    "version": "6.0.0"
  }
}
